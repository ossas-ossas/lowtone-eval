{
  "version": "2.0",
  "envId": "your-cloud-env-id",
  "framework": {
    "name": "express",
    "version": "4.18.2"
  },
  "functions": [
    {
      "name": "api",
      "runtime": "Nodejs16.13",
      "entry": "index.js",
      "timeout": 30,
      "memorySize": 128,
      "environment": {
        "NODE_ENV": "production"
      },
      "triggers": [
        {
          "type": "http",
          "parameters": {
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "path": "/*"
          }
        }
      ]
    }
  ],
  "static": {
    "root": "public",
    "upload": true
  },
  "database": {
    "instanceId": "your-db-instance-id"
  },
  "storage": {
    "bucket": "your-storage-bucket"
  }
}
